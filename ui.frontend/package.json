  "scripts": {
    "dev": "webpack-dev-server --config build-tools/config/webpack/webpack.conf.dev.js",
    "dev:code": "webpack --config build-tools/config/webpack/webpack.conf.code.dev.js --info-verbosity none --watch",
    "build": "cross-env NODE_ENV=production node ./build-tools/script/build",
    "build:critical-css": "npm run build -- critical-css",
    "build:dev": "npm run build dev",
    "build:diff": "cross-env node ./build-tools/script/diff2html",
    "preview": "node build-tools/script/preview-server.js",
    "compile:code": "npm run build code",
    "compile:partials": "npm run build partials",
    "compile:html": "npm run build html",
    "clean": "npm run build clean",
    "clean:boilerplate": "node ./build-tools/script/cleanup",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint src --ext .js,.ts --cache",
    "lint:css": "stylelint src/**/*.scss --cache",
    "analyze": "webpack-bundle-analyzer dist/bundlesize-profile.json ./dist/site",
    "prettify": "prettier --write \"**/*.{js,ts,scss,json,yaml,md}\"",
    "precommit": "lint-staged",
    "postinstall": "patch-package",
    "plop": "plop",
    "generate": "sg wizard",
    "storybook": "start-storybook -p 6006 -s src/static --no-version-updates",
    "build-storybook": "build-storybook -o ./dist/storybook -s src/static",
    "deploy": "rsync -avPx dist/site/ neom-webgl@eu.dev.monkapps.com:~/public/ --delete-after"
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,ts,scss,json,yaml,md}": [
        "prettier --write",
        "git add"
      ],
      "src/**/*.{js,ts}": [
        "eslint --ext .js,.ts --cache"
      ],
      "src/**/*.scss": [
        "stylelint --cache"
      ]
    }
  },
