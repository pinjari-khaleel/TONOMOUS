<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:configPropertyInherit="{Boolean}false"
        emailUrl="mailto:?subject=&amp;body={$TEXT} {$URL}"
        facebookUrl="https://www.facebook.com/sharer/sharer.php?u={$URL}"
        introductionCopy="Please take a look at"
        linkedinUrl="https://www.linkedin.com/shareArticle?mini=true&amp;url={$URL}"
        twitterUrl="https://twitter.com/intent/tweet?url={$URL}&amp;text={$TEXT}&amp;via={$VIA}"
        whatsappUrl="whatsapp://send?text={$URL}"/>
</jcr:root>
