<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="M52 Stylized Radio Option"
          sling:resourceType="cq/gui/components/authoring/dialog"
          extraClientlibs="[tonomus.author]">
  <content
      jcr:primaryType="nt:unstructured"
      sling:resourceType="granite/ui/components/coral/foundation/tabs"
      maximized="{Boolean}true">
    <items jcr:primaryType="nt:unstructured">
      <general
          jcr:primaryType="nt:unstructured"
          jcr:title="General"
          sling:resourceType="granite/ui/components/coral/foundation/container"
          margin="{Boolean}true">
        <items jcr:primaryType="nt:unstructured">
          <columns
              jcr:primaryType="nt:unstructured"
              sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
              margin="{Boolean}true">
            <items jcr:primaryType="nt:unstructured">
              <column
                  jcr:primaryType="nt:unstructured"
                  sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                  <id
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                      fieldDescription="Please enter the id."
                      fieldLabel="Id"
                      name="./id"/>
                  <name
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                      fieldDescription="Please enter the name."
                      fieldLabel="Name"
                      required="{Boolean}true"
                      name="./name"/>
                  <label
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                      fieldDescription="Please enter the label."
                      fieldLabel="Label"
                      required="{Boolean}true"
                      name="./label"/>
                  <value
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                      fieldDescription="Please enter the value."
                      fieldLabel="Value"
                      name="./value"/>
                </items>
              </column>
            </items>
          </columns>
        </items>
      </general>
      <advanced
          jcr:primaryType="nt:unstructured"
          jcr:title="Advanced"
          sling:resourceType="granite/ui/components/coral/foundation/container"
          margin="{Boolean}true">
        <items jcr:primaryType="nt:unstructured">
          <columns
              jcr:primaryType="nt:unstructured"
              sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
              margin="{Boolean}true">
            <items jcr:primaryType="nt:unstructured">
              <column
                  jcr:primaryType="nt:unstructured"
                  sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                  <title
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                      fieldDescription="Please enter the title."
                      fieldLabel="Title"
                      name="./title"/>
                </items>
              </column>
            </items>
          </columns>
        </items>
      </advanced>
      <background
          jcr:primaryType="nt:unstructured"
          jcr:title="Image"
          sling:resourceType="granite/ui/components/coral/foundation/container"
          margin="{Boolean}true">
        <items jcr:primaryType="nt:unstructured">
          <columns
              jcr:primaryType="nt:unstructured"
              sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
              margin="{Boolean}true">
            <items jcr:primaryType="nt:unstructured">
              <column
                  jcr:primaryType="nt:unstructured"
                  sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                  <image
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                      allowUpload="{Boolean}false"
                      autoStart="{Boolean}false"
                      class="cq-droptarget"
                      dropZone="{Boolean}true"
                      fieldDescription="Drag and Drop Image"
                      fieldLabel="Image"
                      fileNameParameter="./image/parameter"
                      fileReferenceParameter="./image/src"
                      mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                      multiple="{Boolean}false"
                      name="name"
                      title="Upload Image Asset"
                      useHTML5="{Boolean}true"/>
                  <altImage
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                      fieldDescription="Please enter Image Alt Text."
                      fieldLabel="Image Alt Text"
                      name="./image/alt"/>
                </items>
              </column>
            </items>
          </columns>
        </items>
      </background>
    </items>
  </content>
</jcr:root>
