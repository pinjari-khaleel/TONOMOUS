<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="M28 Radio Option"
          sling:resourceType="cq/gui/components/authoring/dialog"
          extraClientlibs="[tonomus.author]">
  <content
      jcr:primaryType="nt:unstructured"
      sling:resourceType="granite/ui/components/coral/foundation/tabs"
      maximized="{Boolean}true">
    <items jcr:primaryType="nt:unstructured">
      <general
          jcr:primaryType="nt:unstructured"
          jcr:title="General"
          sling:resourceType="granite/ui/components/coral/foundation/container"
          margin="{Boolean}true">
        <items jcr:primaryType="nt:unstructured">
          <columns
              jcr:primaryType="nt:unstructured"
              sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
              margin="{Boolean}true">
            <items jcr:primaryType="nt:unstructured">
              <column
                  jcr:primaryType="nt:unstructured"
                  sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                  <id
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                      fieldDescription="Please enter the id."
                      fieldLabel="Id"
                      name="./id"/>
                  <name
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                      fieldDescription="Please enter the name."
                      fieldLabel="Name"
                      required="{Boolean}true"
                      name="./name"/>
                  <label
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                      fieldDescription="Please enter the label."
                      fieldLabel="Label"
                      required="{Boolean}true"
                      name="./label"/>
                  <title-in-english
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                      fieldDescription="Please enter a title in English for Analytics."
                      fieldLabel="Field title in English"
                      name="./titleInEnglish"
                      required="{Boolean}true"/>
                  <value
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                      fieldDescription="Please enter the value."
                      fieldLabel="Value"
                      name="./value"/>
                  <checked
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                      fieldDescription="Check to have checked field."
                      name="./checked"
                      text="Checked"
                      value="{Boolean}true"
                      uncheckedValue="{Boolean}false"/>
                  <required
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                      fieldDescription="Check to have required field."
                      name="./required"
                      text="Required"
                      value="{Boolean}true"
                      uncheckedValue="{Boolean}false"/>
                </items>
              </column>
            </items>
          </columns>
        </items>
      </general>
    </items>
  </content>
</jcr:root>
